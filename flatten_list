aList = [1, [2, 3], [4, 5, 6]]

def flatten(aList):
    newList = []
    if aList == []:
        return newList
    else:
        if aList[0] == list:
            newList = newList.pop(aList[0])
        else:
            newList = [aList[0]]
        print newList # testing output
        return newList + flatten(aList[1:])
